@model IList<IKaan.Model.Biz.Catalog.ProductModel>

@{ 
	//Layout = "_Layout";
	string imageSite = @"http://kaangrim.whoisimg.com";
}

@if (Model==null || Model.Count==0){

}
else{
	<div class="page product-tag-page">
		@foreach (var product in Model)
		{
			<img data-name="image" src="@(imageSite + product.ImageUrl)" />
		}
	</div>
}